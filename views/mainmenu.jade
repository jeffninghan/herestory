extends layout

block content
	h1= title
	p Welcome to #{title}

	div(class="container")
		form(role="form" action="test2")
			input(type="submit" style="background:transparent; border:none; color:transparent")
			each x, y in {100:50, 100:200} 
				button(type="submit" class="btn btn-default" style="position:fixed; top:x; left:y") Submit
